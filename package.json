    "@angular/platform-browser": "~8.2.8",
    "@angular/platform-browser-dynamic": "~8.2.8",
    "@angular/router": "~8.2.8",
    "@ngrx/effects": "^8.5.1",
    "@ngrx/router-store": "^8.4.0",
    "@ngrx/store": "^8.4.0",
    "@ngrx/store-devtools": "^8.4.0",
    "aws-amplify": "^1.2.4",
    "aws-amplify-angular": "^3.1.4",
    "aws-sdk": "^2.573.0",
    "d3": "^5.12.0",
    "hammer": "^0.0.5",
